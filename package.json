{
  "name": "waddle-infra",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "infrastructure"
  ],
  "scripts": {
    "synth": "npm run synth --workspace=infrastructure",
    "deploy": "npm run deploy --workspace=infrastructure",
    "destroy": "npm run destroy --workspace=infrastructure",
    "synth:secure": "./scripts/run-with-secrets.sh npm run synth",
    "deploy:secure": "./scripts/run-with-secrets.sh npm run deploy",
    "destroy:secure": "./scripts/run-with-secrets.sh npm run destroy",
    "synth:teleport": "./scripts/deploy-via-teleport.sh synth",
    "deploy:teleport": "./scripts/deploy-via-teleport.sh deploy",
    "destroy:teleport": "./scripts/deploy-via-teleport.sh destroy"
  }
}
